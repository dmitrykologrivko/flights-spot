{
  "name": "flights-spot-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "command": "nest start -- --command",
    "command:prod": "node dist/main --command",
    "create-migration": "npm run command migrations:create --",
    "generate-migration": "npm run command migrations:generate --",
    "migrate": "npm run command migrations:run",
    "migrate:prod": "npm run command:prod migrations:run",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config test/jest-e2e.json --runInBand"
  },
  "dependencies": {
    "@nestjs/common": "^6.11.11",
    "@nestjs/config": "0.0.7",
    "@nestjs/core": "^6.11.11",
    "@nestjs/jwt": "^6.1.2",
    "@nestjs/passport": "^6.2.0",
    "@nestjs/platform-express": "^6.11.11",
    "@nestjs/typeorm": "^6.3.4",
    "@usefultools/monads": "^3.0.3",
    "bcrypt": "^3.0.8",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.11.1",
    "dotenv": "^8.2.0",
    "minimist": "^1.2.5",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.18.2",
    "prompt-sync": "^4.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "sqlite3": "^4.2.0",
    "typeorm": "^0.2.25",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.4.1",
    "@nestjs/schematics": "^7.0.1",
    "@nestjs/testing": "^6.11.11",
    "@types/bcrypt": "^3.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/jest": "^24.9.1",
    "@types/minimist": "^1.2.0",
    "@types/node": "^12.12.47",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.10",
    "create-ts-index": "^1.13.3",
    "jest": "^26.1.0",
    "jest-mock-extended": "^1.0.9",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.1",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^3.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^@app/(.*)$": "<rootDir>/$1",
      "^@core/(.*)$": "<rootDir>/core/$1",
      "^@auth/(.*)$": "<rootDir>/auth/$1"
    },
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
