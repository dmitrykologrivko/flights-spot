{
  "moduleFileExtensions": ["js", "json", "ts"],
  "moduleNameMapper": {
    "^@app/(.*)$": "<rootDir>/../src/$1",
    "^@core/(.*)$": "<rootDir>/../src/core/$1",
    "^@auth/(.*)$": "<rootDir>/../src/auth/$1"
  },
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  }
}
